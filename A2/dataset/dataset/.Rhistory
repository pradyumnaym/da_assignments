add_lines(y = ~y1, name = "A") %>%
add_lines(y = ~y2, name = "B", visible = F) %>%
layout(
title = "Drop down menus - Styling",
xaxis = list(domain = c(0.1, 1)),
yaxis = list(title = "y"),
updatemenus = list(
